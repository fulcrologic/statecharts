{:paths   ["src/main"]

 :deps    {com.taoensso/timbre                         {:mvn/version "6.0.4"}
           metosin/malli                               {:mvn/version "0.13.0"}
           com.fulcrologic/guardrails                  {:mvn/version "1.2.9"}
