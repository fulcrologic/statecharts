{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "env": {}
    },
    "clojure-mcp": {
      "type": "stdio",
      "command": "/bin/sh",
      "args": [
        "-c",
        "/opt/homebrew/bin/clojure -X:mcp"
      ],
      "env": {}
    }
  }
}
